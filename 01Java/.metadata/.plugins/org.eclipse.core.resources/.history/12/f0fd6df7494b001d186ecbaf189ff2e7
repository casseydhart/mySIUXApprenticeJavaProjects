package com.ust;
//Homework Question 1
//write a method that accepts an age and returns true if the age is >= 21
//Test a true and a false condition
public class MethodsPracticeOct13_4 {

	public static void main(String[] args) {
		boolean a = legalAge(22);
		System.out.println("The age 21 and up is:" + a);
	}
	public static boolean legalAge(int a) {
		boolean age = (a >= 21);
		return age;
	}
}
